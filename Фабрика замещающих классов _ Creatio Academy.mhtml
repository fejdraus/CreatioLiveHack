.ts-swu { background: linear-gradient(90deg, rgb(13, 46, 78) 0%, rgb(13, 46=
, 78) 50%, rgb(3, 9, 16) 100%); padding: 8px 0px; text-align: center; posit=
ion: relative; z-index: 3; }

.ts-swu__cont { cursor: default; color: rgb(255, 255, 255); font: 600 20px =
/ 1.3 Montserrat; text-transform: uppercase; display: inline-flex; position=
: relative; padding: 0px 80px; }

@media (max-width: 1600px) {
  .ts-swu__cont { font-size: 18px; }
}

@media (max-width: 767px) {
  .ts-swu__cont { font-size: 15px; line-height: 22px; margin: 0px -10px; }
  .ts-swu__cont span { display: none; }
}

@media (max-width: 360px) {
  .ts-swu__cont { font-size: 12px; margin: 0px -15px; }
}

.ts-swu__cont::before { content: ""; background: url("../../img/i-left.svg"=
) left center no-repeat scroll transparent; position: absolute; left: 0px; =
top: 50%; transform: translateY(-50%); height: 22px; width: 70px; }

.ts-swu__cont::after { content: ""; background: url("../../img/i-right.svg"=
) right center no-repeat scroll transparent; position: absolute; right: 0px=
; top: 50%; transform: translateY(-50%); height: 22px; width: 70px; }

.ts-swu__cont a { text-decoration: underline; color: inherit !important; }

.ts-swu__cont a:hover { text-decoration: none; }

.ts-swu__link { pointer-events: none; text-decoration: none !important; }

@media (max-width: 767px) {
  .ts-swu__link { pointer-events: auto; text-decoration: underline !importa=
nt; }
  .ts-swu__link:hover { text-decoration: none !important; }
}
